	movb	#adm_b_rel,(r9)+	; byte relative
	movb	#2,(r9)+		; (pc+2)
	movb	#adm_adec_r11,(r9)+	; -(r11)
	movb	#op_rsb,(r9)+		; `rsb'
	movb	(r11),(r9)+		; move top of stack into dictionary
