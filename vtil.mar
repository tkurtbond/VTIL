op_tstl		= ^xd5
;
; addressing modes -- constants for compiling addressing modes into dictionary.
;	In order by register and addressing mode.
;
adm_r0		= ^x50			; r0
adm_r9		= ^x59			; r9
adm_def_r9	= ^x69			; (r9)
adm_ainc_r9	= ^x89			; (r9)+
