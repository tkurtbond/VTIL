;
; Dictionary
;
;*******************************************************************************
	.psect	vtildict
c_dict_begin:
	. = .+<16 * 1024>		; reserve space for dictinary~~~ why not a constant
c_dict_end:


;
; Block i/o buffers -- Here because that is the safest place for them.
;
v_blkbuff:
	.blkb	c_max_blocks * c_block_size ; buffer for block i/o


